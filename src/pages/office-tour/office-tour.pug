.pages-home.inner-page
  Loader(v-show='loading')
  div(v-if='props')
    Header
    #interior
      .subnav-outer.mobile
        .container.row
          p
            router-link(to='/treatment-options') Treatment Options
            |  | 
            router-link(to='/office-tour') Office Tour
            |  | 
            router-link(to='/testimonials') Testimonials
            |  | 
            router-link(to='/patient-forms') Patient Forms
            |  | 
            router-link(to='/online-services') Online Services
      .container.row.interior-content
        header
          h1(v-if='props.acf.heading' v-html='props.acf.heading')        
        #main-content2(v-if='props' v-html='props.content.rendered')
        .office-slider
          swiper(:options='swiperOption')
            swiper-slide.slidee(v-for='homeSlider in props.acf.office_tour_slider')
              img.slider-home-img(v-if='homeSlider.tour_image' v-bind:src='homeSlider.tour_image')
            .swiper-pagination.swiper-pagination-bullets(slot='pagination')
            .swiper-button-prev(slot='button-prev') Prev
            .swiper-button-next(slot='button-next') Next           
    Footer
      
